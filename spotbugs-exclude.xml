<FindBugsFilter>
    <Match>
        <!-- Generated by Protobuf -->
        <Class name="~com.ocient.jdbc.proto.*"/>
    </Match>
    <!--False positive. The result set will be cleaned up by the application.-->
    <Match>
        <Class name="com.ocient.jdbc.XGDatabaseMetaData" />
        <Bug pattern="OBL_UNSATISFIED_OBLIGATION" />
    </Match>    
    <!--False positive. I am returning this statement to the cache.-->
    <Match>
        <Class name="com.ocient.jdbc.XGStatement" />
        <Method name="redirect" />
        <Bug pattern="OBL_UNSATISFIED_OBLIGATION" />
    </Match>
    <!--False positive, we do throw general exception.-->
    <Match>
        <Class name="com.ocient.jdbc.XGStatement" />
        <Method name="sendAndReceive" />
        <Bug pattern="REC_CATCH_EXCEPTION" />
    </Match>
    <!--False positive, we do mean to call run()-->
    <Match>
        <Class name="com.ocient.jdbc.XGConnection$TestConnectionThread" />
        <Method name="run" />
        <Bug pattern="RU_INVOKE_RUN" />
    </Match>
    <!--No it shouldn't need to be static inner class.-->
    <Match>
        <Class name="com.ocient.jdbc.XGConnection$XGTrustManager" />
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC" />
    </Match>
    <!--We do intend to take user input.-->
    <Match>
        <Class name="com.ocient.jdbc.KML" />
        <Method name="outputGeospatial" />
        <Bug pattern="PATH_TRAVERSAL_IN" />
    </Match>
    <!--We do intend to take user input.-->
    <Match>
        <Class name="com.ocient.cli.CLI" />
        <Method name="source" />
        <Bug pattern="PATH_TRAVERSAL_IN" />
    </Match>
    <!--We do intend to take user input.-->
    <Match>
        <Class name="com.ocient.cli.CLI" />
        <Method name="outputResultSet" />
        <Bug pattern="PATH_TRAVERSAL_OUT" />
    </Match>
    <!--We do intend to take user input.-->
    <Match>
        <Class name="com.ocient.cli.extract.RecordExtractorFactory" />
        <Method name="create" />
        <Bug pattern="PATH_TRAVERSAL_OUT" />
    </Match>    
    <!--We do intend to take user input.-->
    <Match>
        <Class name="com.ocient.cli.CLI" />
        <Method name="outputGeospatial" />
        <Bug pattern="PATH_TRAVERSAL_IN" />
    </Match>
    <!--This is the CLI specifically taking input from the user.-->
    <Match>
        <Class name="com.ocient.cli.CLI" />
        <Bug pattern="SQL_INJECTION_JDBC" />
    </Match>
    <!--Taking a file from user input is exactly what the tracing wants to do-->
    <Match>
        <Class name="com.ocient.jdbc.TraceProcessor" />
        <Method name="main" />
        <Bug pattern="PATH_TRAVERSAL_IN" />
    </Match>
    <!--I prefer specific filters over broad filters. The next three are
    the same type. We are taking query input from the user.-->
    <Match>
        <Class name="com.ocient.jdbc.XGPreparedStatement" />
        <Method name="execute" />
        <Bug pattern="SQL_INJECTION_JDBC" />
    </Match>
    <Match>
        <Class name="com.ocient.jdbc.XGPreparedStatement" />
        <Method name="executeUpdate" />
        <Bug pattern="SQL_INJECTION_JDBC" />
    </Match>
    <Match>
        <Class name="com.ocient.jdbc.XGPreparedStatement" />
        <Method name="executeQuery" />
        <Bug pattern="SQL_INJECTION_JDBC" />
    </Match>
    <!--Same as XGPreparedStatement. We are taking user input.-->
    <Match>
        <Class name="com.ocient.jdbc.XGStatement" />
        <Method name="execute" />
        <Bug pattern="SQL_INJECTION_JDBC" />
    </Match>
    <!--CIPHER_INTEGRITY,PADDING_ORACLE, and STATIC_IV are associated with the old AES/CBC handshake which
    we've since replaced with AES/GCM/NoPadding.-->
    <Match>
        <Class name="com.ocient.jdbc.XGConnection" />
        <Method name="clientHandshakeCBC" />
        <Bug pattern="CIPHER_INTEGRITY,PADDING_ORACLE,STATIC_IV" />
    </Match>
    <!--Unencrypted socket, but that is why we have a TLS option.-->
    <Match>
        <Class name="com.ocient.jdbc.XGConnection" />
        <Method name="connect" />
        <Bug pattern="UNENCRYPTED_SOCKET" />
    </Match>
    <Match>
        <Class name="com.ocient.jdbc.XGConnection" />
        <Method name="isSockConnected" />
        <Bug pattern="UNENCRYPTED_SOCKET" />
    </Match>
    <!--This is worried about us storing a reference rather than a copy. But we are just writing it out, so its fine.-->
    <Match>
        <Class name="com.ocient.cli.extract.wrappers.Base64ByteArrayWrapper" />
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>
    <Match>
        <Class name="com.ocient.cli.extract.wrappers.UTF8ByteArrayWrapper" />
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>
    <Match>
        <Class name="com.ocient.cli.extract.wrappers.HexadecimalByteArrayWrapper" />
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>     

</FindBugsFilter>
